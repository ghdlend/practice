        int[] array = {10,30,3,70,2};
        int max = array[0];
        int min = array[0];
        for(int i =1;i<array.length;i++){
            if(max < array[i]){
                max = array[i];
            }
            if(min > array[i]){
                min = array[i];
            }
        }
        System.out.println("최대값 : "+max+"최소값 : "+min);